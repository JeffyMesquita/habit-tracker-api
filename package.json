	"scripts": {
		"build": "npx nest build",
		"format": "npx prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "npx nest start",
		"start:dev": "npx nest start --watch",
		"start:debug": "npx nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "npx eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"check": "tsc --noEmit",
		"test": "npx vitest run",
		"test:watch": "npx vitest",
		"test:cov": "npx vitest run --coverage",
		"test:debug": "npx vitest --inspect-brk",
		"test:e2e": "npx vitest run --config ./test/jest-e2e.json",
		"db:gen": "npx prisma generate",
		"db:generate": "npx prisma generate",
		"db:validate": "npx prisma validate",
		"db:push": "npx prisma db push",
		"db:pull": "npx prisma db pull",
		"db:format": "npx prisma format",
		"db:seed": "npx ts-node prisma/seed.ts",
		"db:reset": "npx prisma db push --force-reset && npm run db:seed",
		"docker:up": "docker-compose up -d",
		"docker:down": "docker-compose down",
		"docker:build": "docker-compose build",
		"docker:logs": "docker-compose logs -f",
		"docker:restart": "docker-compose restart app",
		"docker:setup": "docker-compose up -d db && timeout /t 10 /nobreak && npm run db:push && npm run db:seed",
		"docker:setup:unix": "docker-compose up -d db && sleep 10 && npm run db:push && npm run db:seed"
	},
